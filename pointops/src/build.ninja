ninja_required_version = 1.3
cxx = c++
nvcc = /usr/bin/nvcc

cflags = -DTORCH_EXTENSION_NAME=pointops_cuda -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /data2/wangyuansong/miniconda3/envs/idpt/lib/python3.7/site-packages/torch/include -isystem /data2/wangyuansong/miniconda3/envs/idpt/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -isystem /data2/wangyuansong/miniconda3/envs/idpt/lib/python3.7/site-packages/torch/include/TH -isystem /data2/wangyuansong/miniconda3/envs/idpt/lib/python3.7/site-packages/torch/include/THC -isystem /data2/wangyuansong/miniconda3/envs/idpt/include/python3.7m -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++14
post_cflags = 
cuda_cflags = -DTORCH_EXTENSION_NAME=pointops_cuda -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /data2/wangyuansong/miniconda3/envs/idpt/lib/python3.7/site-packages/torch/include -isystem /data2/wangyuansong/miniconda3/envs/idpt/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -isystem /data2/wangyuansong/miniconda3/envs/idpt/lib/python3.7/site-packages/torch/include/TH -isystem /data2/wangyuansong/miniconda3/envs/idpt/lib/python3.7/site-packages/torch/include/THC -isystem /data2/wangyuansong/miniconda3/envs/idpt/include/python3.7m -D_GLIBCXX_USE_CXX11_ABI=0 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr -gencode=arch=compute_75,code=sm_75 --compiler-options '-fPIC' -gencode=arch=compute_75,code=sm_75 -std=c++14
cuda_post_cflags = 
cuda_dlink_post_cflags = 
ldflags = -shared -L/data2/wangyuansong/miniconda3/envs/idpt/lib/python3.7/site-packages/torch/lib -lc10 -lc10_cuda -ltorch_cpu -ltorch_cuda_cu -ltorch_cuda_cpp -ltorch -ltorch_python -L/usr/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags



rule link
  command = $cxx $in $ldflags -o $out

